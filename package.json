{
  "name": "finances",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ignore-path .gitignore **/*.{js,ts,vue}",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "firebase": "^9.18.0"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.3.1",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/i18n": "^8.0.0-beta.10",
    "@pinia/nuxt": "^0.4.7",
    "@vue/test-utils": "^2.3.1",
    "eslint": "^8.36.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "lint-staged": "^13.2.0",
    "nuxt": "^3.3.1",
    "nuxt-icon": "^0.3.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "vitest": "^0.29.3",
    "yorkie": "^2.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix --ignore-path .gitignore"
    ],
    "tests/**/*.{spec,test}.{js,ts}": [
      "vitest --run"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "engines": {
    "node": ">=16.19.0",
    "yarn": "^1.22.19"
  }
}
